{
    "name": "codedsultan/job-engine",
    "description": "A Laravel package for chunked data import/export with real-time progress tracking and job status management.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ibraheem Olusegun",
            "email": "codedsultan369@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-medialibrary": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
      "psr-4": {
        "CodedSultan\\JobEngine\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "Codedsultan\\JobEngine\\Tests\\": "tests/"
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "CodedSultan\\JobEngine\\JobEngineServiceProvider"
        ]
      }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./packages/JobEngine"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
      "post-autoload-dump": [
        "@php artisan package:discover --ansi"
      ]
    }
  }
